extends ./layout/layoutIndex

block main
  .top-space
  .row.center-xs
    .col-xs-10
      table
        thead
          tr: th(colspan="3") Usuarios registrados actualmente
        tbody
          tr
            td ID Usuario
            td FacebookID
            td Nombre
          for user in users
            tr.table
              td: p= user._id
              td: p= user.username
              td: p= user.email
